generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tbl_usuario {
  id    Int    @id @default(autoincrement())
  nome  String @db.VarChar(100)
  email String @db.VarChar(100)
  senha String @db.VarChar(100)

  @@map("tbl_usuario")
}
