generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tbl_recsenha {
  id    Int     @id @default(autoincrement())
  token Int
  usado Boolean
  email String  @db.VarChar(100)
}

model tbl_usuario {
  id    Int    @id @default(autoincrement())
  nome  String @db.VarChar(100)
  senha String @db.VarChar(255)
  email String @db.VarChar(100)
}
